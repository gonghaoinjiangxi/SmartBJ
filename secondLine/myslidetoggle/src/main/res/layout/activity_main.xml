<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:background="#44000000"
    android:layout_height="match_parent"
    tools:context="com.itheima.myslidetoggle.MainActivity">

    <com.itheima.myslidetoggle.com.itheima.myslidetoggle.view.MyViewGroup
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.itheima.myslidetoggle.com.itheima.myslidetoggle.view.MyView
            android:layout_width="300dp"
            android:id="@+id/my_view"
            android:layout_height="50dp"
            android:background="@drawable/bg_myview"/>
    </com.itheima.myslidetoggle.com.itheima.myslidetoggle.view.MyViewGroup>
</RelativeLayout>
